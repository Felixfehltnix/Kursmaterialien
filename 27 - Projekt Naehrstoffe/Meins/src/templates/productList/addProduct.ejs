<tr class="product-tablerow-element" data-fdc="<%= fdcId %>">
    <td>
        <span><%= title %></span>
    </td>
    <td>
        <input class="product-amount product-search_amount" data-fdc="<%= fdcId %>" type="number" value="100" min="0"/>
        <button type="button"
                class="close product_search_addProduct-remove"
                data-dismiss="alert"
                aria-label="Close"
                data-fdc="<%= fdcId %>"
        >
            <span aria-hidden="true">&times;</span>
        </button>
    </td>
</tr>